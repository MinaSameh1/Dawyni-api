{
	"name": "finalproject-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "nodemon ./bin/www",
		"prettier": "prettier -w .",
		"pre-commit": "lint-staged"
	},
	"pre-commit": [
		"pre-commit"
	],
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"arrowParens": "avoid",
		"endOfLine": "lf",
		"singleQuote": true,
		"bracketSpacing": true,
		"trailingComma": "none",
		"tabWidth": 2,
		"semi": false
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.1",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^16.0.0",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"jade": "~1.11.0",
		"morgan": "~1.9.1",
		"nodemon": "^2.0.15"
	},
	"devDependencies": {
		"eslint": "^8.9.0",
		"eslint-plugin-prettier": "^4.0.0",
		"lint-staged": "^12.3.4",
		"pre-commit": "^1.2.2",
		"prettier": "^2.5.1"
	}
}
