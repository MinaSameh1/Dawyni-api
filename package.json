    "dayjs": "^1.10.7",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15",
    "pino": "^7.6.5",
    "pino-pretty": "^7.5.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
