{
  "name": "finalproject-api",
  "version": "1.0.0",
  "description": "Grad Project api",
  "main": "build/index.js",
  "web": "node build/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --max_old_space_size=512 ./build/src/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "debug": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles ",
    "test:watch": "cross-env NODE_ENV=development jest --watch --detectOpenHandles",
    "test:debug": "cross-env NODE_ENV=development node --inspect ./node_modules/.bin/jest --detectOpenHandles --runInBand --watchAll=false",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --ext .ts ./src --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "keywords": [
    "typescript",
    "express",
    "zod",
    "typegoose",
    "mongoose"
  ],
  "author": "Mina Sameh",
  "license": "MIT",
  "dependencies": {
    "@firebase/app-compat": "^0.1.19",
    "@firebase/app-types": "^0.7.0",
    "@typegoose/typegoose": "^9.7.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-pino-logger": "^7.0.0",
    "firebase-admin": "^10.0.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.5",
    "multer": "^1.4.4",
    "openapi-types": "^10.0.0",
    "pino": "^7.8.1",
    "pino-pretty": "^7.5.3",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "tesseract.js": "^2.1.5",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.179",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint_d": "^11.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
